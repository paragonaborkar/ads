<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.talend.master.ads_data_no_git</groupId>
    <artifactId>code.Master</artifactId>
    <version>6.4.0</version>
    <relativePath>./pom.xml</relativePath>
  </parent>
  <groupId>org.talend.job.ads_data_no_git</groupId>
  <artifactId>DeleteTemporaryFiles</artifactId>
  <version>0.1.0</version>
  <name>ADS_DATA_NO_GIT DeleteTemporaryFiles (Job Designs)</name>
  <properties>
    <output.resources.dir>${talend.job.name}/src/main/resources</output.resources.dir>
    <talend.job.bat.classpath>.;../lib/routines.jar;../lib/dom4j-1.6.1.jar;../lib/jakarta-oro-2.0.8.jar;../lib/log4j-1.2.16.jar;deletetemporaryfiles_0_1.jar;</talend.job.bat.classpath>
    <talend.job.applyContextToChildren>false</talend.job.applyContextToChildren>
    <talend.job.version>0.1</talend.job.version>
    <talend.job.sh.addition></talend.job.sh.addition>
    <talend.project.name.lowercase>ads_data_no_git</talend.project.name.lowercase>
    <talend.job.path>ads_data_no_git/deletetemporaryfiles_0_1</talend.job.path>
    <talend.project.name>ADS_DATA_NO_GIT</talend.project.name>
    <talend.product.version>6.4.0.20170510_1410</talend.product.version>
    <talend.job.finalName>deletetemporaryfiles_0_1</talend.job.finalName>
    <talend.job.jvmargs>-Xms256M -Xmx1024M</talend.job.jvmargs>
    <talend.project.branch></talend.project.branch>
    <talend.job.context>Default</talend.job.context>
    <talend.job.stat>true</talend.job.stat>
    <talend.job.date>2018-03-23 124551219</talend.job.date>
    <current.resources.dir>${basedir}/src/main/resources</current.resources.dir>
    <talend.job.package>ads_data_no_git.deletetemporaryfiles_0_1</talend.job.package>
    <talend.job.name>DeleteTemporaryFiles</talend.job.name>
    <talend.job.class>${talend.job.package}.${talend.job.name}</talend.job.class>
    <talend.job.bat.addition></talend.job.bat.addition>
    <talend.job.id>_sb9OoBK3EeiGnZNriZGEXw</talend.job.id>
    <talend.job.sh.classpath>.:$ROOT_PATH:$ROOT_PATH/../lib/routines.jar:$ROOT_PATH/../lib/dom4j-1.6.1.jar:$ROOT_PATH/../lib/jakarta-oro-2.0.8.jar:$ROOT_PATH/../lib/log4j-1.2.16.jar:$ROOT_PATH/deletetemporaryfiles_0_1.jar:</talend.job.sh.classpath>
    <talend.project.id>0</talend.project.id>
    <talend.job.framework></talend.job.framework>
    <talend.job.type>Standard</talend.job.type>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>dom4j-1.6.1</artifactId>
      <version>6.0.0</version>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>jakarta-oro-2.0.8</artifactId>
      <version>6.0.0</version>
    </dependency>
    <dependency>
      <groupId>org.talend.libraries</groupId>
      <artifactId>log4j-1.2.16</artifactId>
      <version>6.0.0</version>
    </dependency>
  </dependencies>
  <build>
    <resources>
      <resource>
        <directory>${project.build.sourceDirectory}</directory>
        <includes>
          <include>**/*.wsdl</include>
        </includes>
      </resource>
    </resources>
    <finalName>${talend.job.finalName}</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <includes>
            <include>${talend.job.path}/**.java</include>
          </includes>
          <testExcludes>
            <exclude>**</exclude>
          </testExcludes>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <skip>true</skip>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <executions>
          <execution>
            <id>copy-running-resources</id>
            <phase>compile</phase>
            <goals>
              <goal>copy-resources</goal>
            </goals>
            <configuration>
              <outputDirectory>${project.build.outputDirectory}/${talend.job.path}</outputDirectory>
              <resources>
                <resource>
                  <directory>${current.resources.dir}/${talend.job.path}</directory>
                  <includes>
                    <include>contexts/*.properties</include>
                  </includes>
                </resource>
              </resources>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <executions>
          <execution>
            <id>default-jar</id>
            <phase>package</phase>
            <goals>
              <goal>jar</goal>
            </goals>
            <configuration>
              <includes>
                <include>${talend.job.path}/*.class</include>
                <include>${talend.job.path}/**/*.wsdl</include>
              </includes>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
